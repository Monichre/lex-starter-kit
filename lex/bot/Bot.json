{
    "name": "lex-starter-kit",
    "description": "A simple chatbot based on AWS Lex and Lambda",
    "locale": "en-US",
    "voiceId": "Joey",
    "childDirected": false,
    "idleSessionTTLInSeconds": 300,
    "abortStatement": {
        "messages": [
            {
                "content": "Sorry, I don't understand.",
                "contentType": "PlainText"
            }
        ]
    },
    "clarificationPrompt": {
        "maxAttempts": 5,
        "messages": [
            {
                "content": "Can try you saying that again?",
                "contentType": "PlainText"
            }
        ]
    },
    "intents": [
        {
            "intentVersion": "$LATEST",
            "intentName": "WhatTime"
        }
    ]
}

{
    "name": "GetStars",
    "sampleUtterances": [
        "How many stars does the project have",
        "How many stars does {Repository} have",
        "How many people have starred {Repository}"
    ],
    "slots": [
        {
            "slotType": "Repository",
            "name": "Repository",
            "slotConstraint": "Required", 
            "valueElicitationPrompt": {
                "maxAttempts": 2,
                "messages": [
                    {
                        "content": "Ok! What is the repository name?",
                        "contentType": "PlainText"
                    }
                ]
            },
            "priority": 1,
            "slotTypeVersion": "$LATEST",
            "sampleUtterances": []
        }
    ],
    "dialogCodeHook": {
        "uri": "arn:aws:lambda:us-east-1:160696617623:function:intentOscarBot",
        "messageVersion": "1.0"
    },
    "fulfillmentActivity": {
        "type": "CodeHook",
        "codeHook": {
            "uri": "arn:aws:lambda:us-east-1:160696617623:function:intentOscarBot",
            "messageVersion": "1.0"
        }
    }
}
